    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.ImagesFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
